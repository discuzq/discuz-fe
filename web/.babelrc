{
  "presets": [
    [
      "next/babel",
      {
        "transform-runtime": {
          "useESModules": false
        },
        "preset-env": {
          "targets": {
            "browsers": [
              ">0.03%"
            ]
          },
          "useBuiltIns": "usage",
          "corejs": 3
        }
      }
    ]
  ],
  "plugins": [
    [
      "@babel/plugin-proposal-decorators",
      {
        "legacy": true
      }
    ],
    [
      "@babel/plugin-proposal-class-properties",
      {
        "loose": true
      }
    ],
    [
      "@discuzq/design-babel-plugin",
      {
        "libraryName": "@discuzq/design",
        "libraryDirectory": "dist/components"
      }
    ]
  ]
}